        "@mui/material": "^7.2.0",
        "axios": "^1.10.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-router-dom": "^7.6.3"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.28.0",
        "@babel/preset-react": "^7.27.1",
        "@eslint/js": "^9.29.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@vitejs/plugin-react": "^4.5.2",
        "babel-jest": "^29.7.0",
        "eslint": "^9.29.0",
