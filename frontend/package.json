{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "docker:build": "docker build -t surveysg-frontend .",
        "docker:run": "docker run -d -p 8080:80 --name frontend surveysg-frontend",
        "docker:stop": "docker stop frontend",
        "docker:restart": "docker restart frontend",
        "docker:logs": "docker logs frontend",
        "docker:clean": "docker rm -f frontend",
        "docker:dev": "npm run docker:clean && npm run docker:build && npm run docker:run",
        "lint": "eslint ."
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.2.0",
